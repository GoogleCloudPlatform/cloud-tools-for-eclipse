             id="com.google.appengine.eclipse.core.gaeNature">
    <runtime>
      <run class="com.google.cloud.tools.eclipse.appengine.compat.DummyNature" />
    </runtime>
  </extension>

  <!-- Dummy GPE GAE runtime type definition; required only for enabling unit testing -->
